{
    "name": "YT Add Block",
    "version": "0.1.0",
    "description": "Block youtube adds",
    "permissions": ["tabs"],
    "host_permissions": ["https://*.youtube.com/*"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://*.youtube.com/*"],
        "js": ["contentScript.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["*.png"],
        "matches": ["https://*.youtube.com/*"]
      }
    ],
    
    "manifest_version": 3
  }